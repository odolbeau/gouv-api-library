{
    "name": "webeweb/geo-api-library",
    "type": "library",
    "description": "Integrate Geo API with your projects",
    "keywords": [
        "adresse",
        "api",
        "commune",
        "departement",
        "library",
        "geo",
        "region",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/geo-api-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0",
        "webeweb/provider-library": "^1.5",
        "webeweb/geo-json-library": "^1.1",
        "webeweb/traits-library": "^2.0",
        "webeweb/types-library": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\GeoAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\GeoAPI\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/geo-api-library/issues"
    }
}
