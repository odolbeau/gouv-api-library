{
    "name": "webeweb/geo-api-library",
    "type": "library",
    "description": "Integrate Geo API with your projects",
    "keywords": [
        "adresse",
        "api",
        "commune",
        "departement",
        "library",
        "geo",
        "region",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/geo-api-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0",
        "webeweb/core-library": "^6.5",
        "webeweb/geo-json-library": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\GeoAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\GeoAPI\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/geo-api-library/issues"
    }
}
